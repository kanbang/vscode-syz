{
  "name": "syz",
  "displayName": "燕姿鼓励师",
  "description": "写代码不再孤单，有孙燕姿与你同在",
  "version": "0.1.1",
  "publisher": "kanbang",
  "icon": "logo.jpg",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "孙燕姿",
    "提醒",
    "鼓励",
    "鼓励师"
  ],
  "bugs": {
    "url": "https://github.com/kanbang/vscode-syz/issues",
    "email": "kanbang@msn.com"
  },
  "homepage": "https://github.com/kanbang/vscode-syz/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/kanbang/vscode-syz.git"
  },
  "activationEvents": [
    "onCommand:syz.showReminderView",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "syz.showReminderView",
        "title": "打开提醒页面",
        "category": "syz"
      },
      {
        "command": "syz.openPhotoFolder",
        "title": "打开照片文件夹，添加自己喜欢的照片",
        "category": "syzfolder"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "孙燕姿",
      "properties": {
        "syz.reminderViewIntervalInMinutes": {
          "type": "number",
          "default": 60,
          "description": "展示提醒页面的时间间隔（分钟）"
        },
        "syz.title": {
          "type": "string",
          "default": "小哥哥，小哥哥，代码写久了，喝杯水吧~",
          "description": "页面内展示文字"
        },
        "syz.chat": {
          "type": "boolean",
          "default": true,
          "description": "启用蝌蚪聊聊"
        },
        "syz.gender": {
          "type": "string",
          "default": "保密",
          "enum": ["男", "女", "保密"],
          "enumDescriptions": [
            "蓝色蝌蚪",
            "粉色蝌蚪",
            "灰色蝌蚪"
          ]
        },
     

        "syz.resLocal": {
          "type": "boolean",
          "default": true,
          "description": "使用本地媒体资源"
        },
        "syz.resWeb": {
          "type": "boolean",
          "default": true,
          "description": "使用网络媒体资源"
        },
        "syz.webResources": {
          "type": "array",
          "default": [
            "https://github.com/kanbang/links/raw/master/pic/1.jpg",
            "https://github.com/kanbang/links/raw/master/pic/2.jpg",
            "https://github.com/kanbang/links/raw/master/video/Stefanie%20Sun/shenqi.mp4",
            "https://github.com/kanbang/links/raw/master/video/Stefanie%20Sun/banjuzaijian.mp4",
            "https://github.com/kanbang/links/raw/master/video/Stefanie%20Sun/shyhda.mp4"
          ],
          "description": "网络资源链接地址，支持[jpg/png/mp4]格式"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "md5-typescript": "^1.0.5",
    "axios": "^0.19.0"
  },
  "__npminstall_done": false
}
