{
    "name": "syz",
    "displayName": "燕姿鼓励师",
    "description": "写代码不再孤单，孙燕姿与你同在",
    "version": "0.1.13",
    "publisher": "kanbang",
    "icon": "logo.jpg",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "孙燕姿",
        "提醒",
        "鼓励",
        "鼓励师",
        "冷门歌手"
    ],
    "bugs": {
        "url": "https://github.com/kanbang/vscode-syz/issues",
        "email": "kanbang@msn.com"
    },
    "homepage": "https://github.com/kanbang/vscode-syz/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/kanbang/vscode-syz.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "kk.syz.remind",
                "title": "打开提醒页面",
                "category": "syz"
            },
            {
                "command": "kk.syz.chat",
                "title": "打开约猿︱媛页面",
                "category": "syz"
            },
            {
                "command": "kk.syz.openPhotoFolder",
                "title": "打开照片文件夹，添加自己喜欢的照片",
                "category": "syzfolder"
            },
            {
                "command": "kk.syz.switch_enable",
                "title": "switch remind enable"
            },
            {
                "command": "kk.syz.set_minutes",
                "title": "set remind minutes"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "孙燕姿",
            "properties": {
                "syz.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "开启提醒"
                },
                "syz.reminderIntervalInMinutes": {
                    "type": "number",
                    "default": 45,
                    "enum": [
                        1,
                        30,
                        45,
                        60,
                        90,
                        120
                    ],
                    "enumDescriptions": [
                        "每[1]分钟提醒一次",
                        "每[30]分钟提醒一次",
                        "每[45]分钟提醒一次",
                        "每[60]分钟提醒一次",
                        "每[90]分钟提醒一次",
                        "每[120]分钟提醒一次"
                    ],
                    "description": "提醒周期（分钟）"
                },
                "syz.title": {
                    "type": "array",
                    "default": [
                        "小哥哥，小哥哥...",
                        "代码写久了，喝杯水休息一下吧..."
                    ],
                    "description": "页面内展示文字"
                },
                "syz.kedou": {
                    "type": "boolean",
                    "default": true,
                    "description": "同时打开蝌蚪聊聊"
                },
                "syz.nickname": {
                    "type": "string",
                    "default": "undefine~",
                    "description": "昵称"
                },
                "syz.gender": {
                    "type": "string",
                    "default": "保密",
                    "enum": [
                        "男",
                        "女",
                        "保密"
                    ],
                    "enumDescriptions": [
                        "蓝色蝌蚪",
                        "粉色蝌蚪",
                        "灰色蝌蚪"
                    ]
                },
                "syz.resLocal": {
                    "type": "boolean",
                    "default": true,
                    "description": "使用本地媒体资源"
                },
                "syz.resWeb": {
                    "type": "boolean",
                    "default": true,
                    "description": "使用网络媒体资源"
                },
                "syz.webResources": {
                    "type": "array",
                    "default": [
                        "https://github.com/kanbang/links/raw/master/pic/1.jpg",
                        "https://github.com/kanbang/links/raw/master/pic/2.jpg"
                    ],
                    "description": "网络资源链接地址，支持[jpg/png]格式"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.55.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.0.0-next.0"
    },
    "dependencies": {
        "md5-typescript": "^1.0.5",
        "axios": "^0.21.1"
    },
    "__npminstall_done": false
}
